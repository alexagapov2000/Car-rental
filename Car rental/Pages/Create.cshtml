@model Car_rental.Models.Car
@{
    ViewBag.Title = "Добавление автомобиля";
}
<form asp-action="create" asp-controller="home">
    <div class="form-group">
        <label asp-for="Id" class="control-label">Название</label>
        <input type="text" asp-for="Id" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Stars" class="control-label">Компания</label>
        <input type="text" asp-for="Stars" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Model" class="control-label">Цена</label>
        <input type="number" asp-for="Model" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>